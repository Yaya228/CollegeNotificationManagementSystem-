<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>MyWebPlus</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>enterr</servlet-name>
		<servlet-class>com.aaaa.Enter</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>getStudentInformation</servlet-name>
		<servlet-class>com.aaaa.getStudentInformation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>getNoticeInformation</servlet-name>
		<servlet-class>com.aaaa.getNoticeInformation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>setNoticeInformation</servlet-name>
		<servlet-class>com.aaaa.setNoticeInformation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>insertNoticeInformation</servlet-name>
		<servlet-class>com.aaaa.insertNoticeInformation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertNoticeInformation</servlet-name>
		<url-pattern>/insertNoticeInformation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>setNoticeInformation</servlet-name>
		<url-pattern>/setNoticeInformation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>getNoticeInformation</servlet-name>
		<url-pattern>/getNoticeInformation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>getStudentInformation</servlet-name>
		<url-pattern>/getStudentInformation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>enterr</servlet-name>
		<url-pattern>/Enter</url-pattern>
	</servlet-mapping>
</web-app>